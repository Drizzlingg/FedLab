#: ../../source/autoapi/fedlab/core/communicator/index.rst:190
msgid ""
"2.1 sender: send a list of slices indicating the size of every content "
"size."
msgstr ""

#: ../../source/autoapi/fedlab/core/communicator/index.rst:173
#: ../../source/autoapi/fedlab/core/communicator/index.rst:192
msgid "2.2 receiver: receive the slices list."
msgstr ""

#: ../../source/autoapi/fedlab/core/communicator/index.rst:175
#: ../../source/autoapi/fedlab/core/communicator/index.rst:194
msgid "3.1 sender: send a content tensor composed of a list of tensors."
msgstr ""

#: ../../source/autoapi/fedlab/core/communicator/index.rst:177
#: ../../source/autoapi/fedlab/core/communicator/index.rst:196
msgid ""
"3.2 receiver: receive the content tensor, and parse it to obtain slices "
"list using parser function"
msgstr ""

